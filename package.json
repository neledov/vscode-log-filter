{
  "name": "vscode-log-filter",
  "displayName": "Log Filter",
  "description": "A VSCode extension to search and filter log files with advanced features.",
  "version": "1.0.0",
  "publisher": "anton neledov",
  "repository": {
    "type": "git",
    "url": "https://github.com/neledov/vscode-log-filter/"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:logSearch.searchLogs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "logSearch.searchLogs",
        "title": "Log Search: Search Logs"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Log Search Configuration",
      "properties": {
        "logSearch.includePatterns": {
          "type": "array",
          "default": ["**/*.log"],
          "description": "Glob patterns to include log files."
        },
        "logSearch.excludePatterns": {
          "type": "array",
          "default": ["**/node_modules/**"],
          "description": "Glob patterns to exclude from search."
        },
        "logSearch.customTimestampRegexes": {
          "type": "array",
          "default": [],
          "description": "Custom regex patterns to extract timestamps."
        },
        "logSearch.timestampFields": {
          "type": "array",
          "default": ["created", "modified"],
          "description": "Fields in JSON logs that contain timestamps."
        },
        "logSearch.keywords": {
          "type": "array",
          "default": [],
          "description": "Keywords to highlight in the search results."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.50.0",
    "typescript": "^4.0.0",
    "vscode-test": "^1.6.0"
  },
  "dependencies": {
    "await-semaphore": "^0.3.2",
    "luxon": "^3.3.0",
    "micromatch": "^4.0.4"
  },
  "keywords": [
    "vscode",
    "extension",
    "log",
    "filter",
    "search"
  ],
  "author": "Anton Neledov",
  "license": "MIT"
}
